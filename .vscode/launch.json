  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "name": "sync run",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/ownershit/main.go",
      "env": {},
      "args": [
        "--config",
        "testrepoconfig.yaml",
        "sync"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "branch run",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/ownershit/main.go",
      "env": {},
      "args": [
        "branches"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch test package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}"
    },
    {
      "name": "run genqlient one",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/genqlient/main.go",
      "cwd": "${workspaceFolder}"
    },
  ]
}
